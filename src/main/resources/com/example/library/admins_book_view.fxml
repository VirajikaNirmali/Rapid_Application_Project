<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.AdminsBookController">
   <children>
      <ImageView fx:id="adminBookImg" fitHeight="509.0" fitWidth="885.0" layoutX="-12.0" layoutY="-9.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img1.jpg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="87.0" prefWidth="750.0" style="-fx-background-color: yellow;" />
      <Label layoutX="303.0" layoutY="15.0" text="Books">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Label>
      <TableView fx:id="bookTable" layoutX="49.0" layoutY="109.0" prefHeight="294.0" prefWidth="652.0">
        <columns>
          <TableColumn fx:id="bookIDColumn" prefWidth="75.0" text="Book ID" />
          <TableColumn fx:id="bookNameColumn" prefWidth="401.0" text="Book Name" />
            <TableColumn fx:id="isAvailableColumn" minWidth="9.0" prefWidth="175.0" text="Is Available" />
        </columns>
      </TableView>
      <Button fx:id="btnHome" layoutX="269.0" layoutY="429.0" mnemonicParsing="false" onAction="#HomeClick" prefHeight="49.0" prefWidth="201.0" text="Home">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
   </children>
</AnchorPane>
