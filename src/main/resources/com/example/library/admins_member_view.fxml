<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.AdminsMemberController">
   <children>
      <ImageView fx:id="adminMemberImg" fitHeight="522.0" fitWidth="773.0" layoutY="-12.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img1.jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="1.0" prefHeight="87.0" prefWidth="772.0" style="-fx-background-color: yellow;" />
      <Label layoutX="267.0" layoutY="19.0" text="Members">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Label>
      <TableView fx:id="memberTable" layoutX="24.0" layoutY="110.0" prefHeight="285.0" prefWidth="701.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colName" prefWidth="166.0" text="Name" />
            <TableColumn fx:id="colEmail" prefWidth="294.0" text="Email" />
            <TableColumn fx:id="colUserRole" prefWidth="165.0" text="User Role" />
        </columns>
      </TableView>
      <Button fx:id="HomeBtn" layoutX="276.0" layoutY="424.0" mnemonicParsing="false" onAction="#HomeClick" prefHeight="56.0" prefWidth="199.0" text="Home">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
   </children>
</AnchorPane>
