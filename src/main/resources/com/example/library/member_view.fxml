<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.MemberViewController">
   <children>
      <ImageView fx:id="MemberDashboardImg" fitHeight="455.0" fitWidth="760.0" layoutY="52.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img3.jpg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="80.0" prefWidth="752.0" style="-fx-background-color: yellow;" />
      <Label layoutX="139.0" layoutY="12.0" text="Member Dashboard">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Label>
      <Button fx:id="btnBooks" layoutX="114.0" layoutY="194.0" mnemonicParsing="false" onAction="#BooksClick" prefHeight="225.0" prefWidth="225.0" style="-fx-background-color: #d24dff;" text="Books">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Button>
      <Button fx:id="btnUpdateProfile" layoutX="419.0" layoutY="194.0" mnemonicParsing="false" onAction="#UpdateProfileClick" prefHeight="225.0" prefWidth="225.0" style="-fx-background-color: #d24dff;" text="Update Profile">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Button>
      <Button fx:id="btnLogout" layoutX="598.0" layoutY="4.0" mnemonicParsing="false" onAction="#LogoutClick" prefHeight="25.0" prefWidth="150.0" text="LogOut" textFill="#da0f0f">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
