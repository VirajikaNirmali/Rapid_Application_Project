<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.AdminsAdminController">
   <children>
      <ImageView fx:id="adminAdminImg" fitHeight="517.0" fitWidth="823.0" layoutY="-10.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img1.jpg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="87.0" prefWidth="775.0" style="-fx-background-color: yellow;">
         <children>
            <Label layoutX="298.0" layoutY="7.0" text="Admins" AnchorPane.rightAnchor="298.0">
               <font>
                  <Font name="System Bold" size="50.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <TableView fx:id="adminTable" layoutX="66.0" layoutY="114.0" prefHeight="268.0" prefWidth="625.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colName" prefWidth="169.0" text="Name" />
            <TableColumn fx:id="colEmail" prefWidth="238.0" text="Email" />
            <TableColumn fx:id="colUserRole" prefWidth="142.0" text="User Role" />
        </columns>
      </TableView>
      <Button fx:id="btnHome" layoutX="268.0" layoutY="411.0" mnemonicParsing="false" onAction="#HomeClick" prefHeight="45.0" prefWidth="214.0" text="Home">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
   </children>
</AnchorPane>
