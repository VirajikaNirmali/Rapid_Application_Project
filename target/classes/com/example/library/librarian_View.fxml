<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.LibrarianViewController">
   <children>
      <AnchorPane prefHeight="80.0" prefWidth="750.0" style="-fx-background-color: yellow;" />
      <ImageView fx:id="librarianDashboardImg" fitHeight="422.0" fitWidth="750.0" layoutY="78.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img2.jpg" />
         </image></ImageView>
      <Label layoutX="140.0" layoutY="14.0" text="Librarian Dashboard">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Label>
      <Button fx:id="membersBtn" layoutX="88.0" layoutY="126.0" mnemonicParsing="false" onAction="#MembersClick" prefHeight="150.0" prefWidth="150.0" style="-fx-background-color: #33ff33;" text="Members">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
      <Button fx:id="adminsBtn" layoutX="524.0" layoutY="126.0" mnemonicParsing="false" onAction="#AdminsClick" prefHeight="150.0" prefWidth="150.0" style="-fx-background-color: #33ff33;" text="Admins">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
      <Button fx:id="bookBtn" layoutX="307.0" layoutY="126.0" mnemonicParsing="false" onAction="#BooksClick" prefHeight="150.0" prefWidth="150.0" style="-fx-background-color: #33ff33;" text="Books">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
      <Button fx:id="eventsBtn" layoutX="195.0" layoutY="313.0" mnemonicParsing="false" onAction="#EventsClick" prefHeight="150.0" prefWidth="150.0" style="-fx-background-color: #33ff33;" text="Events">
         <font>
            <Font name="System Bold" size="20.0" />
         </font></Button>
      <Button fx:id="updataBtn" layoutX="417.0" layoutY="313.0" mnemonicParsing="false" onAction="#UpdateClick" prefHeight="150.0" prefWidth="150.0" style="-fx-background-color: #33ff33;" text="Update Details">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Label fx:id="name" layoutX="15.0" layoutY="4.0" prefHeight="17.0" prefWidth="729.0" textFill="#005ba1">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="btnLogout" layoutX="599.0" layoutY="2.0" mnemonicParsing="false" onAction="#LogoutClick" prefHeight="25.0" prefWidth="150.0" text="LogOut" textFill="#da0f0f">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
