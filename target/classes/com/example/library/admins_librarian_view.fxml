<AnchorPane prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.library.AdminsLibrarianController">
   <children>
      <ImageView fx:id="adminLibrarianImg" fitHeight="515.0" fitWidth="776.0" layoutX="-1.0" layoutY="-15.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../assets/img1.jpg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="87.0" prefWidth="769.0" style="-fx-background-color: yellow;" />
      <Label layoutX="271.0" layoutY="14.0" text="Librarian">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Label>
      <TableView fx:id="tableLibrarian" layoutX="28.0" layoutY="106.0" prefHeight="274.0" prefWidth="693.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="colName" prefWidth="255.0" text="Name" />
            <TableColumn fx:id="colEmail" prefWidth="362.0" text="Email" />
        </columns>
      </TableView>
      <Button fx:id="btnHome" layoutX="270.0" layoutY="414.0" mnemonicParsing="false" onAction="#HomeClick" prefHeight="56.0" prefWidth="209.0" text="Home">
